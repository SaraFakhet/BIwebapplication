{
  "name": "decisionnel",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "mocha ./tests/*.js --timeout 10000 --exit",
    "coverage": "nyc npm test"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "commitlint": "^9.1.2",
    "cookie-parser": "~1.4.4",
    "dateformat": "^3.0.3",
    "debug": "~2.6.9",
    "dotenv-flow": "^3.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "husky": "^4.3.0",
    "mariadb": "^2.4.1",
    "mocha": "^8.1.3",
    "morgan": "^1.9.1",
    "sequelize": "^6.3.3",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "validate-branch-name": "^1.0.6",
    "winston": "^3.3.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "validate-branch-name"
    }
  },
  "validate-branch-name": {
    "pattern": "^(master|develop){1}$|^(feature|fix|hotfix|release)/.+$",
    "errorMsg": "Branch has to start with feature/, fix/, release/ or hotfix/",
    "nyc": "^15.1.0",
    "sequelize": "^6.3.3",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4"
  }
}
